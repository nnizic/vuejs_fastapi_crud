{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      todos: [],\n      newTodo: {\n        title: '',\n        description: ''\n      }\n    };\n  },\n  created() {\n    this.fetchTodos();\n  },\n  methods: {\n    fetchTodos() {\n      axios.get('http://localhost:8000/todos/').then(response => {\n        this.todos = response.data;\n      }).catch(error => {\n        console.error('Error fetching todos:', error);\n      });\n    },\n    createTodo() {\n      const timestamp = Date.now(); // Get current timestamp\n      const randomSuffix = Math.floor(Math.random() * 1000); // Generate random number between 0 and 999\n\n      const randomId = `${timestamp}-${randomSuffix}`; // Combine timestamp and random number\n      axios.post('http://localhost:8000/todos/', this.newTodo).then(response => {\n        this.todos.push(response.data);\n        this.newTodo.id = randomId;\n        this.newTodo.title = '';\n        this.newTodo.description = '';\n      }).catch(error => {\n        console.error('Error creating todo:', error);\n      });\n    },\n    deleteTodo(todoId) {\n      axios.delete(`http://localhost:8000/todos/${todoId}`).then(() => {\n        this.todos = this.todos.filter(todo => todo.id !== todoId);\n      }).catch(error => {\n        console.error('Error deleting todo:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","todos","newTodo","title","description","created","fetchTodos","methods","get","then","response","catch","error","console","createTodo","timestamp","Date","now","randomSuffix","Math","floor","random","randomId","post","push","id","deleteTodo","todoId","delete","filter","todo"],"sources":["/home/giz73/Tests/VueJS/VueJS_FastAPI_CRUD/vue_frontend/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <h1>Todos</h1>\n    <div>\n      <label>Title:</label>\n      <input type=\"text\" v-model=\"newTodo.title\">\n    </div>\n    <div>\n      <label>Description:</label>\n      <input type=\"text\" v-model=\"newTodo.description\">\n    </div>\n    <div>\n      <button @click=\"createTodo\">Create Todo</button>\n    </div>\n    <div v-if=\"todos.length > 0\">\n      <h2>Todos:</h2>\n      <ul>\n        <li v-for=\"todo in todos\" :key=\"todo.id\">\n          <div>{{ todo.title }}</div>\n          <div>{{ todo.description }}</div>\n          <button @click=\"deleteTodo(todo.id)\">Delete</button>\n        </li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      todos: [],\n      newTodo: {\n        title: '',\n        description: ''\n      }\n    }\n  },\n  created() {\n    this.fetchTodos();\n  },\n  methods: {\n    fetchTodos() {\n      axios.get('http://localhost:8000/todos/')\n        .then(response => {\n          this.todos = response.data;\n        })\n        .catch(error => {\n          console.error('Error fetching todos:', error);\n        });\n    },\n    createTodo() {\nconst timestamp = Date.now(); // Get current timestamp\n      const randomSuffix = Math.floor(Math.random() * 1000); // Generate random number between 0 and 999\n\n      const randomId = `${timestamp}-${randomSuffix}`; // Combine timestamp and random number\n      axios.post('http://localhost:8000/todos/', this.newTodo)\n        .then(response => {\n          this.todos.push(response.data);\n          this.newTodo.id=randomId;\n          this.newTodo.title = '';\n          this.newTodo.description = '';\n        })\n        .catch(error => {\n          console.error('Error creating todo:', error);\n        });\n    },\n    deleteTodo(todoId) {\n      axios.delete(`http://localhost:8000/todos/${todoId}`)\n        .then(()=> {\n          this.todos = this.todos.filter(todo => todo.id !== todoId);\n        })\n        .catch(error => {\n          console.error('Error deleting todo:', error);\n        });\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Arial, sans-serif;\n  margin: 20px;\n}\ninput, button {\n  margin: 5px;\n}\n</style>\n\n"],"mappings":";AA4BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE;MACf;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACPD,UAAUA,CAAA,EAAG;MACXR,KAAK,CAACU,GAAG,CAAC,8BAA8B,EACrCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACT,KAAI,GAAIS,QAAQ,CAACV,IAAI;MAC5B,CAAC,EACAW,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C,CAAC,CAAC;IACN,CAAC;IACDE,UAAUA,CAAA,EAAG;MACjB,MAAMC,SAAQ,GAAIC,IAAI,CAACC,GAAG,CAAC,CAAC,EAAE;MACxB,MAAMC,YAAW,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,CAAC,IAAI,IAAI,CAAC,EAAE;;MAEvD,MAAMC,QAAO,GAAK,GAAEP,SAAU,IAAGG,YAAa,EAAC,EAAE;MACjDpB,KAAK,CAACyB,IAAI,CAAC,8BAA8B,EAAE,IAAI,CAACrB,OAAO,EACpDO,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACT,KAAK,CAACuB,IAAI,CAACd,QAAQ,CAACV,IAAI,CAAC;QAC9B,IAAI,CAACE,OAAO,CAACuB,EAAE,GAACH,QAAQ;QACxB,IAAI,CAACpB,OAAO,CAACC,KAAI,GAAI,EAAE;QACvB,IAAI,CAACD,OAAO,CAACE,WAAU,GAAI,EAAE;MAC/B,CAAC,EACAO,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDc,UAAUA,CAACC,MAAM,EAAE;MACjB7B,KAAK,CAAC8B,MAAM,CAAE,+BAA8BD,MAAO,EAAC,EACjDlB,IAAI,CAAC,MAAK;QACT,IAAI,CAACR,KAAI,GAAI,IAAI,CAACA,KAAK,CAAC4B,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACL,EAAC,KAAME,MAAM,CAAC;MAC5D,CAAC,EACAhB,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}